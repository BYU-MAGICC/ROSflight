<?xml version="1.0"?>
<mavlink>
  <include>common.xml</include>
  <enums>
    <!--%%%%%%%%%%%%%%%%<enum name="rosplane_ALT_ZONE_COMMAND">
      <entry value="0" name="ZONE_TAKE_OFF"/>
      <entry value="1" name="ZONE_CLIMB"/>
      <entry value="2" name="ZONE_DESCEND"/>
      <entry value="3" name="ZONE_ALTITUDE_HOLD"/>
    </enum>
    <enum name="rosplane_PATH_TYPES">
      <entry value="0" name="ORBIT_PATH"/>
      <entry value="1" name="LINE_PATH"/>
    </enum>
    <enum name="rosplane_ORBIT_DIRECTION">
      <entry value="-1" name="COUNT_CLOCKWISE"/>
      <entry value="1" name="CLOCKWISE"/>
    </enum>-->
  </enums>
  <messages>
    <!-- abreviated State message for Mavlink -->
    <message id="200" name="MAV_STATE_SMALL">
      <field type="float[3]" name="position">NED position (m)</field>
      <field type="float" name="Va">Airspeed (m/s)</field>
      <field type="float" name="phi">Roll angle (rad)</field>
      <field type="float" name="theta">Pitch angle (rad)</field>
      <field type="float" name="psi">Yaw angle (rad)</field>
      <field type="float" name="chi">Course angle (rad)</field>
      <field type="float" name="initial_lat">Initial/origin latitude (lat. deg)</field>
      <field type="float" name="initial_lon">Initial/origin longitude (lon. deg)</field>
      <field type="float" name="initial_alt">Initial/origin altitude (m)</field>
    </message>

    <!-- MAV Current Path message for Mavlink -->
    <message id="201" name="MAV_CURRENT_PATH">
      <field type="uint8_t" name="flag">Indicates strait line or orbital path (1 = line, 0 = orbit)</field>
      <field type="float" name="Va_d">Desired airspeed (m/s)</field>
      <field type="float[3]" name="r">Vector to origin of straight line path (m)</field>
      <field type="float[3]" name="q">Unit vector, desired direction of travel for line path</field>
      <field type="float[3]" name="c">Center of orbital path (m)</field>
      <field type="float" name="rho">Radius of orbital path (m)</field>
      <field type="int8_t" name="lambda">Direction of orbital path (clockwise is 1, counterclockwise is -1)</field>
    </message>

    <!-- MAV  Waypoint message for Mavlink-->
    <message id="202" name="MAV_WAYPOINT">
      <field type="float[3]" name="w">Waypoint in local NED (m)</field>
      <field type="float" name="chi_d">Desired course at this waypoint (rad)</field>
      <field type="uint8_t" name="chi_valid">Desired course valid (1 = valid, 0 = !valid)</field>
      <field type="float" name="Va_d">Desired airspeed (m/s)</field>
      <field type="uint8_t" name="set_current">Sets this waypoint to be executed now (1 = true, 0 = false)</field>
    </message>

    <!-- RC Raw message for Mavlink -->
    <message id="203" name="RC_RAW">
      <field type="uint16_t[8]" name="values">RC Channel values</field>
    </message>

    <!-- GPS Data message for Mavlink -->
    <message id="204" name="GPS_DATA">
      <field type="uint8_t" name="fix">Status of gps fix (1 = true, 0 = false)</field>
      <field type="uint16_t" name="NumSat">Number of connected satellites</field>
      <field type="float" name="latitude">Latitude (deg)</field>
      <field type="float" name="longitude">Longitude (deg)</field>
      <field type="float" name="altitude">Altitude (m)</field>
      <field type="float" name="speed">Speed (m/s)</field>
      <field type="float" name="ground_course">Ground course (rad)</field>
      <field type="float" name="covariance">Covariance (m)</field>
    </message>

    <!-- Controller Internals message for Mavlink -->
    <message id="205" name="MAV_CONTROLLER_INTERNALS">
      <field type="float" name="theta_c">Commanded pitch (rad)</field>
      <field type="float" name="phi_c">Commanded roll (rad)</field>
      <field type="uint8_t" name="alt_zone">Zone in the latitude state machine</field>
      <field type="float[4]" name="aux">Optional auxiliary commands</field>
      <field type="uint8_t" name="aux_valid">Auxiliary commands valid (1 = true, 0 = false)</field>
    </message>

    <!-- Controller Commands message for Mavlink -->
    <message id="206" name="MAV_CONTROLLER_COMMANDS">
      <field type="float" name="Va_c">Commanded airspeed (m/s)</field>
      <field type="float" name="h_c">Commanded altitude (m)</field>
      <field type="float" name="chi_c">Commanded course (rad)</field>
      <field type="float" name="phi_ff">Feed forward command for orbits (rad)</field>
      <field type="float[4]" name="aux">Optional auxiliary commands</field>
      <field type="uint8_t" name="aux_valid">Auxiliary commands valid (1 = true, 0 = false)</field>
    </message>
  </messages>
</mavlink>
